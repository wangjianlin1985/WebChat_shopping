<view class="container">
  <loading hidden="{{loadingHide}}">
    {{loadingText}}
  </loading>

  <view class="title">商品评价详情</view>
  <view class="evaluate_detail_view"> 
      <view class="viewItem">
        <view class="label">评价编号：</view>
        <view class="content">
          <view>{{evaluateId}}</view>
        </view>
      </view>

       <view class="viewItem">
        <view class="label">商品名称：</view>
        <view class="content"> 
          <view>{{productObj}}</view>
        </view>
      </view>

       <view class="viewItem">
        <view class="label">用户名：</view>
        <view class="content"> 
          <view>{{userObj}}</view>
        </view>
      </view>

       <view class="viewItem">
        <view class="label">评价内容：</view>
        <view class="content"> 
          <view>{{content}}</view>
        </view>
      </view>

       <view class="viewItem">
        <view class="label">评价时间：</view>
        <view class="content"> 
          <view>{{evaluateTime}}</view>
        </view>
      </view>

      <view>
        <navigator class="btnReturn" open-type="navigateBack" hover-class="navigator-hover">返回</navigator>
      </view>

  </view>
</view>
